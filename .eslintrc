{
  "root": true,
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": [
    "standard",
    "standard-react",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "rules": {
    "no-unused-expressions": 0,
    "react/prop-types": [2, { "skipUndeclared": true }],
    "no-console": ["error", { "allow": ["warn", "error"] }]
  }
}
